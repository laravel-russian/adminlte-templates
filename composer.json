{
  "name": "laravel-russian/adminlte-templates",
  "type": "library",
  "description": "AdminLTE templates for LaravelRussian Laravel Generator",
  "keywords": [
    "laravel",
    "generator",
    "adminlte",
    "templates",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mitul Golakiya",
      "email": "me@mitul.me"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0",
    "laravel-russian/laravel-ui-adminlte": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelRussian\\AdminLTETemplates\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\LaravelRussian\\AdminLTETemplates\\AdminLTETemplatesServiceProvider"
      ]
    }
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/laravel-russian"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
